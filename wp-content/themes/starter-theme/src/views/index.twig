{% extends "base.twig" %}

{% block content %}
  {% include 'page-title/page-title.twig' with {
    'title': title,
    'type': 'archive',
  } %}

  {% for post in posts %}
    {% include ['tease-'~ post.post_type ~'.twig', 'tease/tease.twig'] %}
  {% endfor %}

  {% include 'pagination/pagination.twig' with {
    'pagination': posts.pagination,
  } %}
{% endblock %}
